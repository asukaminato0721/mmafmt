{
  "name": "mmafmt",
  "displayName": "mmafmt",
  "description": "fmt code",
  "version": "0.0.2",
  "repository": {
    "url": "whatever"
  },
  "activationEvents": [
    "onLanguage:wolfram",
    "onStartupFinished"
  ],
  "engines": {
    "vscode": "^1.75.0"
  },
  "categories": [
    "Programming Languages",
    "Keymaps"
  ],
  "main": "./extension.js",
  "contributes": {
    "configuration": {
      "title": "MmaFmt"
    },
    "languages": [
      {
        "id": "wolfram",
        "aliases": [
          "Wolfram Language"
        ],
        "extensions": [
          ".wls",
          ".wlnb",
          ".wl",
          ".nb"
        ]
      }
    ]
  }
}